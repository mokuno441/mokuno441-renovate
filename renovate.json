{
  "platform": "github",
  "regexManagers": [
    {
      "depNameTemplate": "mokuno441/renovate-test-repo",
      "matchStrings": ["renovate-test-repo_version=(?<currentValue>.*)"],
      "fileMatch": ["^versions.ini"],
      "datasourceTemplate": "github-releases",
      "versioningTemplate": "semver-coerced"
    },
    {
      "depNameTemplate": "mokuno441/renovate-test-repo-2",
      "matchStrings": ["renovate-test-repo-2_version=(?<currentValue>.*)"],
      "fileMatch": ["^versions.ini"],
      "datasourceTemplate": "github-releases"
    }
  ]
}
